<?xml version="1.0"?>
<opencv_storage>
	<!-- number of inner chessboard corners per row !-->
	<rows>6</rows>
	<!-- number of inner chessboard corners per column !-->
	<columns>5</columns>
	<!-- size of a chessboard square, in an arbitrary measurement unit (centimeters were used in this example) !-->
	<square-size>12</square-size>
	<!-- if != 0, camera intrinsics, distortion coefficients, rotational matrix
			 and translational vector are loaded from matrices.xml. After the stereo
			 set has been calibrated, set this parameter to an integer != 0 to speed
			 up following executions of the program !-->
	<load-matrices>0</load-matrices>
	<!-- path to left image of the scene to be reconstructed !-->
	<input-image-left>
		"left/image_0033.png"
	</input-image-left>
	<!-- path to right image of the scene to be reconstructed !-->
	<input-image-right>
		"right/image_0033.png"
	</input-image-right>
	<!-- paths to calibration images taken by right camera !-->
